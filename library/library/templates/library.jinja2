{% extends "layout.jinja2" %}

{% block content %}

<div class="content">
  <p class="lead"><span class="font-normal"></span>
  <h2>Library listing</h2>
  {% for lib_id, lib_name, lib_address in library_list['library']%}
     <p>{{lib_id}}: <a href=/category?library={{lib_id}}>{{lib_name}}</a>
     {% for user_id, user_fname, user_lname in library_list['subscriber']%}
         <p>&nbsp;&nbsp;&nbsp;&nbsp;{{user_id}}: <a href=/pay?subscriber={{user_id}}>{{user_fname}} {{user_lname}}</a></p>
     {% endfor %}
  {% endfor %}
 </div>
{% endblock content %}
